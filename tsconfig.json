{
  "compilerOptions": {
    "outDir": "dist",
    "target": "ES2016",
    "module": "ESNext",
    "moduleResolution": "nodenext",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "declaration": false,
    "sourceMap": false,
    "resolveJsonModule": true
  },
  "include": ["src/*"]
}
